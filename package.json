    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "esquery": "1.4.0",
    "fastify": "^4.13.0",
    "jest": "^29.4.3",
    "newrelic": "^9.10.1",
    "pino": "^8.10.0",
    "prettier": "^2.8.4",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18"
  }
